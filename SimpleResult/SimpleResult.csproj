<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>

    <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10.0</LangVersion>
    <Authors>Meisam Alifallahi</Authors>
    <Description>Result&lt;T&gt; is a monad for modelling success (Ok) or failure (Err) operations.</Description>
    <PackageId>Arax.SimpleResult</PackageId>
    <Version>1.1.0</Version>
    <PackageProjectUrl>https://github.com/araxis/SimpleResult</PackageProjectUrl>
    <RepositoryUrl>https://github.com/araxis/SimpleResult</RepositoryUrl>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="logo.png" Pack="true" PackagePath="" />
    <None Update="logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>



</Project>
